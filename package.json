{
  "name": "mattermost-redux",
  "version": "0.1.0",
  "main": "build/index.js",
  "module": "build/mattermost-redux.esm.js",
  "typings": "build/index.d.ts",
  "files": [
    "build"
  ],
  "scripts": {
    "start": "tsdx watch -i ./src/client/client4.ts -i ./src/client/websocket_client.ts",
    "build": "tsdx build -i ./src/client/client4.ts -i ./src/client/websocket_client.ts",
    "test": "tsdx test",
    "lint": "eslint 'src/**/*.ts' --fix"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "2.3.1",
    "gfycat-sdk": "latest",
    "moment-timezone": "^0.5.25",
    "isomorphic-fetch": "2.2.1",
    "redux": "^4.0.1",
    "redux-action-buffer": "^1.2.0",
    "redux-offline": "git+https://github.com/enahum/redux-offline.git#4bd85e7e3b279a2b11fb4d587808d583d2b5e7b5",
    "redux-persist": "4.9.1",
    "redux-persist-node-storage": "^2.0.0",
    "redux-thunk": "^2.3.0",
    "remote-redux-devtools": "^0.5.16",
    "reselect": "^4.0.0",
    "serialize-error": "^4.1.0",
    "shallow-equals": "^1.0.0",
    "source-map-support": "^0.5.12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json",
        "diagnostics": true
      },
      "NODE_ENV": "test"
    },
    "rootDir": ".",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.js"
    ],
    "transform": {
      "^.+\\.(ts|tsx|js|jsx)?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "testMatch": [
      "<rootDir>/src/**/?(*.)(spec|test).(ts|js)?(x)"
    ],
    "verbose": true,
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1",
      "^test/(.*)$": "<rootDir>/test/$1",
      "^utils/(.*)$": "<rootDir>/src/utils/$1",
      "^action_types$": "<rootDir>/src/action_types",
      "^constants$": "<rootDir>/src/constants",
      "^action_types/(.*)$": "<rootDir>/src/action_types/$1",
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@types/jest": "^24.0.18",
    "@types/moment-timezone": "^0.5.12",
    "babel-eslint": "^10.0.3",
    "babel-plugin-module-resolver": "^3.2.0",
    "eslint-config-mattermost": "github:mattermost/eslint-config-mattermost#6fe27a2bd1f9df4d4eb96fac6fcdcc0fee0ecbd7",
    "eslint-import-resolver-webpack": "0.11.1",
    "eslint-plugin-cypress": "2.7.0",
    "eslint-plugin-header": "3.0.0",
    "eslint-plugin-import": "2.18.2",
    "form-data": "^2.5.1",
    "husky": "^3.0.5",
    "mock-socket": "^9.0.0",
    "nock": "^11.3.5",
    "redux-mock-store": "^1.5.3",
    "tsdx": "^0.9.2",
    "tslib": "^1.10.0",
    "typescript": "^3.6.3"
  }
}

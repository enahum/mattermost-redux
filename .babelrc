{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "ie": 11,
        "chrome": 43,
        "firefox": 52,
        "edge": 15,
        "ios": 9
      },
      "useBuiltIns": "usage",
      "shippedProposals": true
    }],
    "@babel/preset-flow"
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["./src", "."]
    }],
    "@babel/proposal-class-properties"
  ]
}
